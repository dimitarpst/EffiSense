@model Usage

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Usage</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.User)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.User.Id)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Appliance)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Appliance.ApplianceId)
