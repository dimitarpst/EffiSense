            @Html.DisplayNameFor(model => model.Date)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Date)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.EnergyUsed)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.EnergyUsed)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.UsageFrequency)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.UsageFrequency)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Duration)
